{# app/Resources/vievs/index.html.twig the global index #}
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>{% block title %} BDTLN {% endblock %}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="">
  <meta name="author" content="Anthony Bocci, Jeremy Renaud, Clément Chaussade, Robin Neveu, Arthur Rouchon, Benoît Gay-Bellile, Nicolas Ringuet">
    <!-- Latest compiled and minified CSS -->
    {% block stylesheet %}
        <!--link rel="stylesheet/less" href="{{ asset('bootstrap/less/bootstrap.less') }}" type="text/css" /-->
	<!--link rel="stylesheet/less" href="{{ asset('bootstrap/less/responsive.less') }}" type="text/css" /-->
	<!--script src="{{ asset('bootstrap/js/less-1.3.3.min.js') }}"></script-->
	<!--append ‘#!watch’ to the browser URL, then refresh the page. -->
	
	<link href="{{ asset('bootstrap/css/bootstrap.css') }}" rel="stylesheet">
	<link href="{{ asset('bootstrap/css/style.css') }}" rel="stylesheet">
    {% endblock %}	
  <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
  <!--[if lt IE 9]>
    <script src="{{ asset('bootstrap/js/html5shiv.js') }}"></script>
  <![endif]-->

  <!-- Fav and touch icons -->
  <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('bootstrap/img/apple-touch-icon-144-precomposed.png') }}">
  <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('bootstrap/img/apple-touch-icon-114-precomposed.png') }}">
  <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('bootstrap/img/apple-touch-icon-72-precomposed.png') }}">
  <link rel="apple-touch-icon-precomposed" href="{{ asset('bootstrap/img/apple-touch-icon-57-precomposed.png') }}">
  <link rel="shortcut icon" href="{{ asset('bootstrap/img/favicon.png') }}">
    {% block scripts %}
	<script type="text/javascript" src="{{ asset('bootstrap/js/jquery.min.js') }}"></script>
	<script type="text/javascript" src="{{ asset('bootstrap/js/bootstrap.min.js') }}"></script>
	<script type="text/javascript" src="{{ asset('bootstrap/js/scripts.js') }}"></script>
    {% endblock %}
</head>

<body>

        <div class="container">
            {% include ":header:header.html.twig" %}
    

            {# Here the block named body, it will redefined by the bundle layouts #}
            {% block body %}
               
            {% endblock %}
        
            
            <div class="row clearfix" id="pied_de_page">
                <div class="col-md-12 column">
                <img src="{{ asset("bootstrap/img/footer.png") }}">	
                </div>
            </div>
        </div>
       
          
 
        <script type="text/javascript" src="{{ asset('scripts/change_locale.js') }}">
        </script>
    </body>
</html>
