{% trans_default_domain 'FOSUserBundle' %}

<form id="form"action="{{ path('fos_user_change_password', {'username': user.username}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_change_password">
    {{ form_widget(form) }}
    <div>
        <input type="submit"  class="btn btn-primary" value="{{ 'change_password.submit'|trans }}" />

    </div>
</form>
<button id="back" name="back" class="btn btn-primary" onClick="history.back()">{{ "Back" | trans }}</button>